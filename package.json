{
  "name": "image-tagger",
  "version": "1.0.0",
  "description": "an image tagging system that will help add tags to images.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/server.ts",
    "test": "mocha -r ts-node/register 'src/test/*.test.ts' --timeout 2000ms"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roshankathawate/image-tagger.git"
  },
  "author": "Roshan Kathawate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roshankathawate/image-tagger/issues"
  },
  "homepage": "https://github.com/roshankathawate/image-tagger#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^6.0.6",
    "mongoose-paginate-v2": "^1.4.2",
    "nanoid": "^3.1.25",
    "pino": "^6.13.2",
    "pino-pretty": "^7.0.1",
    "typescript": "^4.4.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.2.22",
    "@types/chai-http": "^4.2.0",
    "@types/config": "0.0.39",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.173",
    "@types/mocha": "^9.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-paginate-v2": "^1.3.11",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.9.2",
    "@types/pino": "^6.3.11",
    "@types/supertest": "^2.0.11",
    "@types/yup": "^0.29.13",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "jest": "^27.2.1",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  }
}
